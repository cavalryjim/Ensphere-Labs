
<!DOCTYPE html>
<html>
<head>
  <title>Ensphere Labs</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="container">
		<div id="header">
			<div id="headercontent">
				<%= render 'layouts/header' %>	
			</div><!-- #headercontent -->
		</div><!-- #header -->
		<div id="content">
			
			<%= yield %>
		</div><!-- #content -->
		<div id="footer">
			<div id="footercontent">
				<%= render 'layouts/footer' %>	
				
				<div class="clear"></div>
			</div><!-- #footercontent -->
		</div><!-- #footer -->		
	</div><!-- #container -->
</body>
</html>
